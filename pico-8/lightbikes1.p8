scrn = {}
p1 = {x=0,y=0,dir=1,dead=false}
p2 = {x=0,y=0,dir=1,dead=false}

function _init()
  show_menu()
end

function show_menu()
  scrn.upd = update_menu
  scrn.drw = draw_menu
end

function run_game()
  scrn.upd = update_game
  scrn.drw = draw_game
  
  p1.x = 32
  p1.y = 64
  p1.dir = 1
  p1.dead = false
 
  p2.x = 96
  p2.y = 64
  p2.dir = 1
  p2.dead = false

   
  cls()
  rect(0,0,127,127,1)
  sfx(0,0,0)
end

function show_game_over()
  scrn.upd = update_game_over
  scrn.drw = draw_game_over
  --sfx(-1,0,0)
  sfx(3,0,0)
end


function update_menu()
 if (btnp(4)) then run_game() end
end

function draw_menu()
  cls()
  spr(0, 0,0, 20,20)
--  print("b      i      k      e      s",6,40,14)
 
  print("speler 1 - s/f toetsen",10,7*9,13)
  print("speler 2 - ”ƒ‹‘ toetsen",10,7*10,15)
  print("   door: michiel erasmus",10,7*12,4)
 
  print("kies Ž om te start",30,100,7)
end

function update_player(player,player_num)
  local last_dir = player.dir
     
  
  if (btnp(0,player_num)) then player.dir = player.dir - 1 end
	 if (btnp(1,player_num)) then player.dir = player.dir + 1 end
	  
	 if (player.dir < 1) then player.dir = 4 end
	 if (player.dir > 4) then player.dir = 1 end

	 if (player.dir == 1) then player.y = player.y - 1 end
	 if (player.dir == 2) then player.x = player.x + 1 end
	 if (player.dir == 3) then player.y = player.y + 1 end
	 if (player.dir == 4) then player.x = player.x - 1 end  

	 if (last_dir != player.dir) then sfx(2,1,0) end

	 if (pget(player.x,player.y) != 0) then 
	   player.dead = true
	   show_game_over() 
	 end
end


function update_game()
  update_player(p1,0)
  update_player(p2,1)
end

function draw_game()
	 pset(p1.x,p1.y,12)
	 pset(p2.x,p2.y,14)
end


function update_game_over()
 if (btnp(4)) then show_menu() end
end

anim = 0

function draw_game_over()
  --cls()
  print("game over !",45,50,7)
  if (p1.dead) then
    circfill(p1.x,p1.y,anim/5,anim)
    print ("speler 2 win",40,60,7)    
  end
  if (p2.dead) then
    circfill(p2.x,p2.y,anim/5,anim)
    print ("speler 1 win",40,60,7)   
  end
  print("kies Ž om door te gaan",25,100,7)
  anim = anim+1
  if anim > 16 then anim = 0 end
end



function _update()
  scrn.upd()
end

function _draw()
  scrn.drw()
end    
